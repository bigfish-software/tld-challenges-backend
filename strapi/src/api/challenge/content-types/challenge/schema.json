{
  "kind": "collectionType",
  "collectionName": "challenges",
  "info": {
    "singularName": "challenge",
    "pluralName": "challenges",
    "displayName": "Challenge"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "blocks"
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "difficulty": {
      "type": "enumeration",
      "default": "Custom",
      "enum": [
        "Pilgrim",
        "Voyager",
        "Stalker",
        "Interloper",
        "Misery",
        "Nogoa",
        "Custom"
      ]
    },
    "created_date": {
      "type": "datetime"
    },
    "updated_date": {
      "type": "datetime"
    },
    "submissions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::submission.submission",
      "mappedBy": "challenge"
    },
    "creators": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::creator.creator",
      "inversedBy": "challenges"
    },
    "faqs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::faq.faq",
      "inversedBy": "challenges"
    },
    "custom_code": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::custom-code.custom-code",
      "inversedBy": "challenges"
    },
    "rules": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::rule.rule",
      "inversedBy": "challenges"
    },
    "tournament": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::tournament.tournament",
      "inversedBy": "challenges"
    }
  }
}

{
  "kind": "collectionType",
  "collectionName": "custom_codes",
  "info": {
    "singularName": "custom-code",
    "pluralName": "custom-codes",
    "displayName": "Custom Code"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": false
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "description_short": {
      "type": "text",
      "maxLength": 160
    },
    "description_long": {
      "type": "blocks"
    },
    "code": {
      "type": "string",
      "required": true
    },
    "is_featured": {
      "type": "boolean",
      "default": false
    },
    "creators": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::creator.creator",
      "mappedBy": "custom_codes"
    },
    "faqs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::faq.faq",
      "inversedBy": "custom_codes"
    },
    "challenges": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::challenge.challenge",
      "mappedBy": "custom_code"
    }
  }
}
